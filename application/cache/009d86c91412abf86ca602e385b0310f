        <script type="text/javascript">

    var calender_diklat = {
        init: function () {
            this._create_table();
        },
        _create_table: function () {
            $("#table_agenda_diklat").dataTable({
//                scrollX: 1100
                serverSide: true,
                ajax: {
                    url: base_url('api/adaftar_diklat/kalender_diklat'), //"http://localhost:8080/github/2016apsidika/api/adaftar_diklat/kalender_diklat",
                },
                columns: [
                    {name: 'nama_diklat', orderable: true},
                    {name: 'tgl_pelaksanaan', orderable: false},
                    {name: 'status', searchable: false},
                    {name: 'action', orderable: false, searchable: false},
                ]
            });
            $("#table_agenda_diklat").on('page.dt', function () {
                onresize(100);
            });
        }
    };

    $(document).ready(function () {

    });
</script><script type="text/javascript">
    $(document).ready(function () {
        calender_diklat.init();
    });
</script>
