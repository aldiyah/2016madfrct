    var escape_value = function (obj) {
        if (obj != null) {
            for(var j in obj){
                var clean_index = [0,1,2,3,4,7,8];
                for(var i in clean_index){
                    obj[j][clean_index[i]]["value"] = cleanValue(obj[j][clean_index[i]]["value"]);
                }
            }
            return obj;
        }
        return obj;
    };

